<h1>Reporte</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Fechareporte</th>
            <td>{{ entity.fechaReporte|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Horasreportadas</th>
            <td>{{ entity.horasReportadas }}</td>
        </tr>
        <tr>
            <th>Comentario</th>
            <td>{{ entity.comentario }}</td>
        </tr>
        <tr>
            <th>Created_at</th>
            <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('reporte') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('reporte_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('reporte_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
